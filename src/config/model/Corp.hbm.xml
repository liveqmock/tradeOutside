<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.github.cruiser.tradeoutside.model">
    <class name="Corp" table="CORP">
        <id name="id" column="corp_id" type="java.lang.Long" access="field">
            <generator class="native"/>
        </id>
        
        <property name="busiNo" not-null="true" unique="true"/>
		<property name="dccRate"/>
		<property name="edcRate"/>

		<set name="dccTerminals" table="DCCTERMINALS">
			<key column="id" foreign-key="id" />
			<element type="string" column="dccTerm" />
		</set>

		<set name="edcTerminals" table="EDCTERMINALS">
			<key column="id" foreign-key="id" />
			<element type="string" column="edcTerm" />
		</set>

    </class>
</hibernate-mapping>