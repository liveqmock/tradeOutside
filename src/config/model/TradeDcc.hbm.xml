<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.github.cruiser.tradeoutside.model">
    <class name="TradeDcc" table="TRADEDCC">
        <id name="id" column="tradedcc_id" type="java.lang.Long" access="field">
            <generator class="native"/>
        </id>
        
        <property name="txnTim" not-null="true"/>
		<property name="txnCod" not-null="true"/>
		<property name="seqNum" not-null="true"/>
		<property name="termId" not-null="true"/>
		<property name="acpAdr" not-null="true"/>
		<property name="actNo" not-null="true"/>
		<property name="valDat" not-null="true"/>
		<property name="txnAmt" not-null="true"/>
		<property name="tips" not-null="true"/>
		<property name="aRspCd" not-null="true"/>
		<property name="txnDat" not-null="true"/>
		<property name="busiNo" not-null="true"/>
		<property name="crdTyp" not-null="true"/>
		<property name="actDat" not-null="true"/>
        
    </class>
</hibernate-mapping>